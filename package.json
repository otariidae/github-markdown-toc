{
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "@typescript-eslint/parser": "^1.1.0",
    "@webcomponents/custom-elements": "^1.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "event-target-shim": "^4.0.3",
    "fuse-box": "^3.6.0",
    "h5o": "^0.11.3",
    "immer": "^1.1.3",
    "jsdom": "^13.0.0",
    "kocha": "^1.8.0",
    "lit-html": "^1.0.0",
    "npm-run-all": "^4.1.0",
    "nyc": "^13.3.0",
    "postcss-cli": "^6.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.7.2",
    "primer-support": "^5.0.0",
    "sass": "^1.15.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "name": "github-markdown-toc",
  "scripts": {
    "clean": "rm -rf dist",
    "postclean": "mkdir dist",
    "build:css": "sass -I ./node_modules src/css/index.scss dist/index.css",
    "postbuild:css": "postcss dist/index.css --output dist/index.css",
    "build:js": "node fuse.js",
    "build:meta": "node --experimental-modules ./scripts/manifest.mjs",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "prettier": "prettier --write \"{src/**,scripts,test,modules/*}/*.{mjs,js,ts,tsx}\"",
    "prelint:js": "npm run prettier",
    "lint:js": "eslint src scripts --ext .mjs,.js",
    "prelint:ts": "npm run prettier",
    "lint:ts": "eslint src modules --ext .ts,.tsx --fix",
    "lint:css": "prettier --write src/css/*.scss",
    "lint:md": "prettier --write *.md",
    "lint": "run-p lint:*",
    "test": "cross-env NODE_ENV=test kocha --require ts-node/register test/*.js modules/*/test.js",
    "precov": "nyc npm test",
    "cov": "nyc report --reporter=json",
    "preversion": "run-p lint test",
    "version": "npm run build:meta"
  },
  "version": "0.12.0",
  "license": "CC0-1.0",
  "private": true,
  "repository": "otariidae/github-markdown-toc",
  "bugs": {
    "url": "https://github.com/otariidae/github-markdown-toc/issues"
  },
  "nyc": {
    "exclude": [
      "postcss.config.js"
    ]
  },
  "prettier": {
    "semi": false
  },
  "dependencies": {}
}
